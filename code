		Set Ammo(Event Player, 0, 50);
		Wait Until(!Is Using Ability 1(Event Player) || Is Using Ultimate(Event Player), 9999);
		Set Ammo(Event Player, 0, 50);
		Remove Health Pool From Player(Event Player.health_pool[0]);
	}
}

