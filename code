			Remove From Array(All Living Players(Team 1), Event Player), Has Spawned(Current Array Element))) * 0.800);
		Abort If(!Is Game In Progress);
		Event Player.knockback_value += 150;
	}
}

rule("Mauga")
{
	event
	{
		Ongoing - Each Player;
		Team 1;
		Mauga;
	}

	actions
	{
		Event Player.max_health_percent = 100;
